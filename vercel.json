{
    "version": 2,
    "builds": [
      {
        "src": "pages/**/*.js",
        "use": "@vercel/next"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  